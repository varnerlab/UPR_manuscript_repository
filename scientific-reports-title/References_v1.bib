	Journal-Full = {Proceedings of the National Academy of Sciences of the United States of America},
	Mesh = {Amino Acid Sequence; Animals; Calcium-Binding Proteins; Calreticulin; Computer Simulation; Models, Molecular; Molecular Sequence Data; Nuclear Magnetic Resonance, Biomolecular; Protein Structure, Tertiary; Rats; Ribonucleoproteins},
	Month = {Mar},
	Pages = {3133-8},
	Pmc = {PMC30619},
