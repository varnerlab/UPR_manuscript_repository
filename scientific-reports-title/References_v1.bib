	Mesh = {Animals; Cells, Cultured; DNA-Binding Proteins; Fibroblasts; Hela Cells; Humans; Mice; Mice, Knockout; Nuclear Proteins; Protein Conformation; RNA Splicing; Recombinant Fusion Proteins; Transcription Factors; Ubiquitin},
	Month = {Mar},
	Pages = {5852-60},
