	Mesh = {Activating Transcription Factor 6; DNA-Binding Proteins; Endopeptidases; Endoplasmic Reticulum; Fluorescent Antibody Technique; Glycosylation; Hela Cells; Humans; Membrane Glycoproteins; Molecular Weight; Nuclear Proteins; Protein Folding; Protein Processing, Post-Translational; Solubility; Thapsigargin; Transcription Factors; Transfection; Tunicamycin},
	Month = {Nov},
	Pages = {3787-99},
	Pmid = {10564271},
	Title = {Mammalian transcription factor ATF6 is synthesized as a transmembrane protein and activated by proteolysis in response to endoplasmic reticulum stress},
	Volume = {10},
	Year = {1999}}

