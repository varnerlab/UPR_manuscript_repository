	Journal = {Mol Cancer Res},
	Journal-Full = {Molecular cancer research : MCR},
	Mesh = {Cell Hypoxia; Eukaryotic Initiation Factor-4F; Humans; Neoplasms; Phosphorylation; Protein Biosynthesis; Protein Folding; Tumor Suppressor Proteins; eIF-2 Kinase},
	Month = {Jul},
	Pages = {423-36},
	Pmid = {16849518},
	Title = {"Translating" tumor hypoxia: unfolded protein response (UPR)-dependent and UPR-independent pathways},
	Volume = {4},
	Year = {2006},
	Bdsk-Url-1 = {http://dx.doi.org/10.1158/1541-7786.MCR-06-0150}}

