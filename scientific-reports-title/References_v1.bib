	Month = {Jun},
	Pages = {2961-8},
	Pmc = {PMC450237},
	Pmid = {8670797},
	Pst = {ppublish},
	Title = {Calnexin and calreticulin promote folding, delay oligomerization and suppress degradation of influenza hemagglutinin in microsomes},
	Volume = {15},
	Year = {1996}}

@article{Hebert:2005ve,
	Abstract = {The majority of proteins that traverse the secretory pathway receive asparagine (Asn)-linked glycosylations. Glycans are bulky hydrophilic modifications that serve a variety of structural and functional roles within the cell. Here, we review the recent growing knowledge on the role of Asn-linked glycans as maturation and quality-control protein tags in the early secretory pathway. The carbohydrate composition encodes crucial information about the structure, localization and age of glycoproteins. The "glycan code" is encoded by a series of glycosidases and carbohydrate transferases that line the secretory pathway. This code is deciphered by carbohydrate-binding proteins that possess distinct carbohydrate binding properties and act as molecular chaperones or sorting receptors. These glycosidases and transferases work in concert with resident secretory pathway carbohydrate-binding proteins to form a network that assists in the maturation and trafficking of both native and aberrant glycoproteins within the cell.},
	Author = {Hebert, Daniel N and Garman, Scott C and Molinari, Maurizio},
	Date-Added = {2010-12-14 19:32:59 -0500},
	Date-Modified = {2010-12-14 19:32:59 -0500},
