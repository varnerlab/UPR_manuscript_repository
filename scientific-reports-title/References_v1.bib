	Mesh = {Activating Transcription Factor 6; Animals; Cells, Cultured; Endoplasmic Reticulum; Intracellular Signaling Peptides and Proteins; Mice; Mice, Transgenic; Models, Biological; Models, Statistical; Muscle Proteins; Myocardium; Oligonucleotide Array Sequence Analysis; RNA; RNA, Small Interfering; Rats; Signal Transduction},
	Month = {May},
	Pages = {14012-21},
	Pmid = {18319259},
	Title = {Coordination of growth and endoplasmic reticulum stress signaling by regulator of calcineurin 1 (RCAN1), a novel ATF6-inducible gene},
	Volume = {283},
