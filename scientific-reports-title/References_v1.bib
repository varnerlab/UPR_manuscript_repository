	Journal = {J Am Soc Nephrol},
	Journal-Full = {Journal of the American Society of Nephrology : JASN},
	Mesh = {Animals; Anti-Bacterial Agents; Disease Models, Animal; Endoplasmic Reticulum; Enzyme Inhibitors; Glomerulonephritis, Membranoproliferative; Heat-Shock Proteins; Ischemic Preconditioning; Isoantibodies; Kidney; Male; Mice; Mice, Inbred BALB C; Molecular Chaperones; Oxidative Stress; Proteins; Rats; Rats, Wistar; Signal Transduction; Thapsigargin; Tunicamycin},
	Month = {May},
