	Journal = {Cancer Res},
	Journal-Full = {Cancer research},
	Mesh = {Animals; Caspases, Initiator; Endoplasmic Reticulum; Enzyme Activation; Enzyme Inhibitors; Glioma; HIV Protease Inhibitors; Heat-Shock Proteins; Humans; Male; Mice; Mice, Nude; Molecular Chaperones; Nelfinavir; Neoplasm Transplantation; Oligopeptides; Pyridines; Transcription Factor CHOP},
	Month = {Nov},
	Pages = {10920-8},
	Pmid = {18006837},
	Title = {HIV-1 protease inhibitors nelfinavir and atazanavir induce malignant glioma death by triggering endoplasmic reticulum stress},
	Volume = {67},
