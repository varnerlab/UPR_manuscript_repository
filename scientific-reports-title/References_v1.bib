	Mesh = {Algorithms; Animals; Diabetes Mellitus; Endoplasmic Reticulum; Humans; Insulin-Secreting Cells; Mice; Models, Theoretical; Phenotype; Predictive Value of Tests; Proinsulin; Protein Folding; Signal Transduction},
	Pages = {233-44},
