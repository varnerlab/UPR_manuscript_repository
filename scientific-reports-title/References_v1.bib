	Journal-Full = {The Journal of biological chemistry},
	Mesh = {Cell Line; DNA-Binding Proteins; Databases as Topic; Endoplasmic Reticulum; Glycoproteins; Glycosylation; Humans; Mannosidases; Membrane Proteins; Nuclear Proteins; Peptides; Protein Binding; Protein Folding; Protein Sorting Signals; Protein Structure, Tertiary; Reverse Transcriptase Polymerase Chain Reaction; Subcellular Fractions; Time Factors; Transcription Factors; Transcription, Genetic; Transfection; Up-Regulation; alpha-Mannosidase},
	Month = {Jan},
	Pages = {2424-8},
