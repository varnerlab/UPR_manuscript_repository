	Journal-Full = {Biochemical and biophysical research communications},
	Mesh = {Cloning, Molecular; Endoplasmic Reticulum; Fungal Proteins; HSP70 Heat-Shock Proteins; Membrane Glycoproteins; Models, Biological; Molecular Chaperones; Phosphorylation; Protein Folding; Protein-Serine-Threonine Kinases; Recombinant Proteins; Saccharomyces cerevisiae; Saccharomyces cerevisiae Proteins; Signal Transduction},
	Month = {Dec},
	Pages = {445-50},
