	Mesh = {Aminoglycosides; Animals; Anti-Bacterial Agents; Antibiotics, Antineoplastic; Cell Hypoxia; Cell Survival; DNA Damage; Enediynes; Humans; Mice; Oxygen; Zinostatin},
	Month = {Jul},
	Pages = {S52-6},
	Pmid = {8763846},
	Title = {Oxygen dependence of the cytotoxicity of the enediyne anti-tumour antibiotic esperamicin A1},
	Volume = {27},
	Year = {1996}}

