	Journal = {Genes Dev},
	Journal-Full = {Genes \& development},
	Mesh = {3T3 Cells; Amino Acid Sequence; Animals; Base Sequence; Blotting, Western; CCAAT-Enhancer-Binding Proteins; Cell Division; DNA; DNA-Binding Proteins; Immunohistochemistry; Leucine Zippers; Mice; Molecular Sequence Data; Nuclear Proteins; Plasmids; Precipitin Tests; RNA, Messenger; Sequence Alignment; Transcription Factor CHOP; Transcription Factors; Transcription, Genetic; Transfection},
	Month = {Mar},
	Pages = {439-53},
	Pmid = {1547942},
	Title = {CHOP, a novel developmentally regulated nuclear protein that dimerizes with transcription factors C/EBP and LAP and functions as a dominant-negative inhibitor of gene transcription},
