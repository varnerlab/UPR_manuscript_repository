	Doi = {10.1002/jnr.10312},
	Journal = {J Neurosci Res},
	Journal-Full = {Journal of neuroscience research},
	Mesh = {Alzheimer Disease; Amyloid beta-Protein Precursor; Animals; Apoptosis; Brefeldin A; Caspases; Cells, Cultured; Cyclooxygenase Inhibitors; Endoplasmic Reticulum; Enzyme Inhibitors; Fetus; Golgi Apparatus; Membrane Proteins; Mice; Mice, Transgenic; Mutation; Neurons; Nordihydroguaiaretic Acid; Oxidative Stress; Presenilin-1; Protein Folding; Protein Synthesis Inhibitors; Signal Transduction; Staurosporine},
	Month = {Aug},
	Pages = {530-9},
	Pmid = {12210846},
	Title = {Neurons overexpressing mutant presenilin-1 are more sensitive to apoptosis induced by endoplasmic reticulum-Golgi stress},
