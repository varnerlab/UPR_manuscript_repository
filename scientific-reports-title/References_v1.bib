	Journal-Full = {PloS one},
	Mesh = {Adenosine Triphosphate; Animals; Calcium; Calcium Signaling; Computational Biology; Humans; Kinetics; Models, Biological; Models, Statistical; Models, Theoretical; Neurons, Afferent; Pain; Receptors, Purinergic P2; Sensory Receptor Cells; Signal Transduction},
	Number = {9},
