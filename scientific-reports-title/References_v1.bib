	Doi = {10.1074/jbc.M509409200},
	Journal = {J Biol Chem},
	Journal-Full = {The Journal of biological chemistry},
	Mesh = {Animals; Autophagy; Cells, Cultured; Endoplasmic Reticulum; Inclusion Bodies; Mice; Microtubule-Associated Proteins; Mutation; alpha 1-Antitrypsin},
	Month = {Feb},
	Pages = {4467-76},
	Pmid = {16365039},
	Title = {Intracellular inclusions containing mutant alpha1-antitrypsin Z are propagated in the absence of autophagic activity},
	Volume = {281},
