	Month = {Sep},
	Pages = {5183-94},
	Pmid = {17785575},
	Title = {Nelfinavir, A lead HIV protease inhibitor, is a broad-spectrum, anticancer agent that induces endoplasmic reticulum stress, autophagy, and apoptosis in vitro and in vivo},
