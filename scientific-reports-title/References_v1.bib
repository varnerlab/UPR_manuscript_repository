	Mesh = {Animals; Animals, Newborn; Anoxia; Basic-Leucine Zipper Transcription Factors; Cells, Cultured; DNA-Binding Proteins; Gene Expression Regulation; Heat-Shock Proteins; Mice; Molecular Chaperones; Myocardial Infarction; Myocytes, Cardiac; Neoplasm Proteins; Protein Folding; RNA Splicing; Rats; Transcription Factors},
	Month = {Aug},
