	Mesh = {Animals; Endoplasmic Reticulum; Glucosyltransferases; Glycoproteins; Protein Conformation; Protein Folding; Rats; Thermodynamics},
	Month = {Oct},
	Pages = {46280-5},
	Pmid = {15319428},
	Pst = {ppublish},
	Title = {The endoplasmic reticulum glucosyltransferase recognizes nearly native glycoprotein folding intermediates},
	Volume = {279},
