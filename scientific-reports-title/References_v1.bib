	Volume = {42},
	Year = {2003}}

@article{arnaudeau2002cdm,
	Abstract = {To study the role of calreticulin in Ca(2+) homeostasis and apoptosis, we generated cells inducible for full-length or truncated calreticulin and measured Ca(2+) signals within the cytosol, the endoplasmic reticulum (ER), and mitochondria with "cameleon" indicators. Induction of calreticulin increased the free Ca(2+) concentration within the ER lumen, [Ca(2+)](ER), from 306 +/- 31 to 595 +/- 53 microm, and doubled the rate of ER refilling. [Ca(2+)](ER) remained elevated in the presence of thapsigargin, an inhibitor of SERCA-type Ca(2+) ATPases. Under these conditions, store-operated Ca(2+) influx appeared inhibited but could be reactivated by decreasing [Ca(2+)](ER) with the low affinity Ca(2+) chelator N,N,N',N'-tetrakis(2-pyridylmethyl)ethylenediamine. In contrast, [Ca(2+)](ER) decreased much faster during stimulation with carbachol. The larger ER release was associated with a larger cytosolic Ca(2+) response and, surprisingly, with a shorter mitochondrial Ca(2+) response. The reduced mitochondrial signal was not associated with visible morphological alterations of mitochondria or with disruption of the contacts between mitochondria and the ER but correlated with a reduced mitochondrial membrane potential. Altered ER and mitochondrial Ca(2+) responses were also observed in cells expressing an N-truncated calreticulin but not in cells overexpressing calnexin, a P-domain containing chaperone, indicating that the effects were mediated by the unique C-domain of calreticulin. In conclusion, calreticulin overexpression increases Ca(2+) fluxes across the ER but decreases mitochondrial Ca(2+) and membrane potential. The increased Ca(2+) turnover between the two organelles might damage mitochondria, accounting for the increased susceptibility of cells expressing high levels of calreticulin to apoptotic stimuli.},
	Author = {Arnaudeau, Serge and Frieden, Maud and Nakamura, Kimitoshi and Castelbou, Cyril and Michalak, Marek and Demaurex, Nicolas},
	Date-Added = {2010-12-14 19:32:59 -0500},
	Date-Modified = {2010-12-14 19:32:59 -0500},
	Doi = {10.1074/jbc.M202395200},
	Journal = {J Biol Chem},
	Journal-Full = {The Journal of biological chemistry},
	Mesh = {Apoptosis; Blotting, Western; Calcium; Calreticulin; Cell Line; Cytosol; Endoplasmic Reticulum; Homeostasis; Humans; Immunohistochemistry; Membrane Potentials; Mitochondria},
	Month = {Nov},
	Pages = {46696-705},
	Pmid = {12324449},
	Title = {Calreticulin differentially modulates calcium uptake and release in the endoplasmic reticulum and mitochondria},
	Volume = {277},
	Year = {2002},
	Bdsk-Url-1 = {http://dx.doi.org/10.1074/jbc.M202395200}}

