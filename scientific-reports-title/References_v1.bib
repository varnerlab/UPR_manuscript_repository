	Mesh = {Adenine; Animals; Autophagy; Caspase 12; Cell Death; Endoplasmic Reticulum; Enzyme Activation; Eukaryotic Initiation Factor-2; Gene Expression Regulation; Leucine; Lysosomes; Mice; Microtubule-Associated Proteins; Models, Biological; Pepstatins; Peptides; Phosphorylation; Protein Structure, Quaternary; RNA, Messenger; Sirolimus; eIF-2 Kinase},
	Month = {Feb},
