	Doi = {10.1091/mbc.E03-11-0851},
	Journal = {Mol Biol Cell},
	Journal-Full = {Molecular biology of the cell},
	Mesh = {Adaptor Proteins, Signal Transducing; Animals; Azetidinecarboxylic Acid; Base Sequence; Cell Line; Cell Survival; DNA, Complementary; Endoplasmic Reticulum; Enzyme Activation; MAP Kinase Signaling System; Membrane Proteins; Mice; Mitogen-Activated Protein Kinase 3; Models, Biological; Oncogene Proteins; Protein-Serine-Threonine Kinases; Rats; Recombinant Fusion Proteins; Tunicamycin},
	Month = {Sep},
	Pages = {4248-60},
	Pmc = {PMC515356},
