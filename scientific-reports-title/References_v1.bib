	Journal = {FASEB J},
	Journal-Full = {The FASEB journal : official publication of the Federation of American Societies for Experimental Biology},
	Mesh = {Animals; Blotting, Western; Calcimycin; Calcium; Cell Line; HSP70 Heat-Shock Proteins; Homeostasis; Ionophores; Membrane Proteins; Mice; Myocardial Ischemia; Myocytes, Cardiac; Necrosis; Rats; Transfection},
	Month = {May},
