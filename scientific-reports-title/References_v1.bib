	Journal = {Mol Cell Biol},
	Journal-Full = {Molecular and cellular biology},
	Mesh = {3T3 Cells; Amino Acid Sequence; Animals; Apoptosis; Cell Differentiation; Cell Division; Cricetinae; Gene Expression; Genes, p53; Growth Inhibitors; Humans; Mammals; Mice; Molecular Sequence Data; Proteins; Sequence Homology, Amino Acid; Species Specificity; Transcription, Genetic; Transfection; Tumor Cells, Cultured},
	Month = {Apr},
